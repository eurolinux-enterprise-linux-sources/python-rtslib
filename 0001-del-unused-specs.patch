-
-# The fabric module feature set
-features = ("acls",)
-
-# Non-standard module naming scheme
-kernel_module = "ib_srpt"
-
-# Transform 'fe80:0000:0000:0000:0002:1903:000e:8acd' WWN notation to
-# '0xfe8000000000000000021903000e8acd'
-def wwns():
